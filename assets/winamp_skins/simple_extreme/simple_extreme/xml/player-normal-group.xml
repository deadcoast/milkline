<!-- Song File Information and Visualization Group Definition -->
<groupdef id="player.normal" name="Winamp">

  <layer id="numbg"
    x="-226" y="8"
	relatx="1" 
	ghost="1" 
	image="num.bg"
	alpha="25"
  />

  <!--Text Info-->
  <text
	id="timer"
	display="time"
	x="-227" y="4"
	relatx="1" 
	w="88" h="33"
	font="player.BIGNUM3" fontsize="0" align="right"
	timecolonwidth="7"
  /><!--player.BIGNUM/21/w="70" h="18"/x="83" y="7"/timecolonwidth="7"-->
    
  <!-- Visualzation -->  
  <layer id="net"
    x="0" y="0" 
	ghost="1" 
	image="net"
  />
  <vis
	id="vis"
	x="7" y="6"
	w="71" h="41"
	alpha="196"
	
	colorband1="255,255,255"
	colorband2="255,255,255"
	colorband3="255,255,255"
	colorband4="255,255,255"
	colorband5="235,235,235"
	colorband6="235,235,235"
	colorband7="235,235,235"
	colorband8="235,235,235"
	colorband9="215,215,215"
	colorband10="215,215,215"
	colorband11="215,215,215"
	colorband12="215,215,215"
	colorband13="195,195,195"
	colorband14="195,195,195"
	colorband15="195,195,195"
	colorband16="195,195,195"
	colorbandpeak="255,189,48"
	colorosc1="175,175,175"
	colorosc2="215,215,215"
	colorosc3="255,255,255"
	colorosc4="215,215,215"
	colorosc5="175,175,175"  
	tooltip="Visualization"
  />
  
  <layer id="netmask"
    x="0" y="0" 
	ghost="1" 
	image="net.mask"
  />

  <!-- Song File Information-->
  <text
	id="infoline" ticker="1"
	display="songinfo" default=""
	x="85" y="38"
	w="100" h="11"
	font="a_LCDNova" fontsize="15" ghost="1"
  />
  <!-- Glass-->
  <layer id="glassleft"
    x="0" y="0" w="188" h="53"
	ghost="1" 
	image="glass.left"
  />
</groupdef>

<groupdef id="player.volume" name="Volume">
<!-- Volume -->
  <layer
    id="volumebar"
    x="35" y="16"
    w="1"
    image="volume.line"
  />
  
  <slider
    id="Volume"
    action="VOLUME"
    x="29" y="11"
    w="112" h="13"
    thumb="volume.base"
    downThumb="volume.pressed"
    hoverThumb="volume.hover"
    tooltip="Volume"
  />
  
  <button
	id="mute"
	x="5" y="5"
	image="mute.base"
	downImage="mute.pressed"
    hoverImage="mute.hover"
	tooltip="Toggle Mute"
  />
  
  <button
	id="mutec"
	x="5" y="5"
	image="mute.base_c"
	downImage="mute.pressed_c"
    hoverImage="mute.hover_c"
	tooltip="Toggle Mute"
  />
  
  <!-- Songticker -->
  <text
    id="Songticker" ticker="1"
    display="songname" showlen="1"
    x="0" y="36"
    w="141" h="20"
    leftpadding="6"
    rightpadding="6"
    font="player.songticker.font"
    align="center"
    valign="top"
    rightclickaction="trackmenu"
    dblclickaction="trackinfo"
    fontsize="13"
    color="255,255,255"
    altfont="a_LCDNova"
	antialias="1"
  />
  <!--text
	font="player.songticker.font" fontsize="16"
  /-->
  <script id="mute.script" file="scripts/mute.maki" />

	<!-- Volume -->
</groupdef>