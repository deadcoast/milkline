<groupdef id="Sysmenu" name="Sysmenu">
  <!-- Sysmenu -->
  <button
    id="shadeSysmenu"
    action="SYSMENU"
    x="0" y="0"
    image="logo.base"
    downImage="logo.pressed"
    hoverImage="logo.hover"
    tooltip="Main Menu"
	inactiveAlpha="150"
  />
</groupdef>

<!-- Window Buttons Group Definition -->
<groupdef id="player.normal.captionbuttons" name="Caption buttons">
	<button
		id="Minimize"
		action="MINIMIZE"
		x="42" y="7"
		image="minimize.base"
		downImage="minimize.pressed"
		hoverImage="minimize.hover"
		tooltip="Minimize"
	/>
	
	<button
		id="Close"
		action="CLOSE"
		x="62" y="7"
		image="close.base"
		downImage="close.pressed"
		hoverImage="close.hover"
		tooltip="Close Winamp"
	/>
	
	<layer id="Minimize_i"
		x="42" y="7"
		ghost="1" 
		image="minimize.base_i"
		alpha="0"
		inactiveAlpha="255"
	/>
	
	<layer id="Close_i"
		x="62" y="7"
		ghost="1" 
		image="close.base_i"
		alpha="0"
		inactiveAlpha="255"
	/>
</groupdef>

<!-- Winamp Caption Group Definition -->
<groupdef id="Caption" name="WinAmp Caption">
  <text
    id="songticker" ticker="1"
	display="songname" default="" showlen="1"
	x="0" y="0"
	w="216" h="14"
	font="wasabi.font.default" fontsize="13" ghost="1"
  />
</groupdef>

<!-- Playback Progress Group Definition -->
<groupdef id="SeekerBar" name="Song Progress">
  <ProgressGrid
	w="-5" h="3" relatw="1"
    orientation="right"
    x="0" y="3"
    middle="seeker.line"
  />
  
  <Slider
   id="Seeker"
   action="SEEK"
   x="0" y="0" 
   w="-2" h="9"
   relatw="1"
   thumb="player/seeker/seeker_base.png"
   downThumb="player/seeker/seeker_pressed.png"
   hoverthumb="player/seeker/seeker_hover.png"
   orientation="horizontal"  tooltip="Seek"
  />
</groupdef>

<!-- Playback Buttons Group Definition -->
<groupdef id="player.normal.playbuttons" name="Player buttons">
	<button
		id="Eject"
		action="EJECT"
		x="0" y="13"
		image="player.button.openbase"
		downImage="player.button.openpressed"
		hoverImage="player.button.openhover"
		tooltip="Open"
	/>
	<button
		id="Stop"
		action="STOP"
		x="34" y="13"
		image="player.button.stopbase"
		downImage="player.button.stoppressed"
		hoverImage="player.button.stophover"
		tooltip="Stop"
	/>
	<button 
		id="Previous"
		action="PREV" 
		x="68" y="13" 
		image="player.button.previousbese" 
		downImage="player.button.previouspressed"
		hoverImage="player.button.previoushover"
		tooltip="Previous"
	/>
	<button
		id="Next"
		action="NEXT"
		x="160" y="13"
		image="player.button.nextbese"
		downImage="player.button.nextpressed"
		hoverImage="player.button.nexthover"
		tooltip="Next"
	/>
	<layer id="glow"
		x="114" y="0" 
		alpha="0"
		ghost="1"
		image="player.button.glow"	 
	/>	
	<button
		id="Play"
		action="PLAY"
		x="114" y="0"
		image="player.button.playbase" 
		downImage="player.button.playpressed"
		hoverImage="player.button.playhover"
		tooltip="Play"
	/>
	<button
		id="Pause"
		action="PAUSE"
		x="114" y="0"
		image="player.button.pausebase"
		downImage="player.button.pausepressed"
		hoverImage="player.button.pausehover"
		tooltip="Pause"
	/>
	<togglebutton
		id="Repeat"
		x="221" y="13"
		image="repeat.base"
		downImage="repeat.pressed"
		hoverImage="repeat.hover"
		activeImage="repeat.base_c"
		cfgattrib="{45F3F7C1-A6F3-4EE6-A15E-125E92FC3F8D};Repeat"
		cfgval="2"
		tooltip="Repeat"
	/>
	<togglebutton
		id="Shuffle"
		x="255" y="13"
		image="shuffle.base"
		downImage="shuffle.pressed"
		hoverImage="shuffle.hover"
		activeImage="shuffle.base_c"
		cfgattrib="{45F3F7C1-A6F3-4EE6-A15E-125E92FC3F8D};Shuffle"
		tooltip="Shuffle"
	/>
	<script id="play2pause" file="scripts/glow.maki"/>
	<script id="play2pause" file="scripts/play2pause.maki"/>
</groupdef>

